game.Players.LocalPlayer.Chatted:connect(function(msg)
time = string.sub(msg, 9)
 if msg == ".cmds" then
function notify(msg)     game.StarterGui:SetCore("SendNotification", {
Title = "NTO'S ADMIN";         
Text = msg;         
Duration = 6;     
}) 
end
notify(".fly .etp .speed .jump .crash .cmds .clr .clear .admin .crash")
wait(2)
 end
end)

game.Players.LocalPlayer.Chatted:connect(function(msg)
time = string.sub(msg, 9)
 if msg == ".qtp" then 
 loadstring(game:HttpGet("https://pastebin.com/raw/CEpwwzDM"))()
function notify(msg)     game.StarterGui:SetCore("SendNotification", {
Title = "NTO'S ADMIN";         
Text = msg;         
Duration = 3;     
}) 
end
notify("Q to teleport activated")
 end
end)

game.Players.LocalPlayer.Chatted:connect(function(msg)
time = string.sub(msg, 9)
 if msg == ".fly" then
loadstring(game:HttpGet("https://pastebin.com/raw/7rXZ9VNc", true))()
 end
end)

game.Players.LocalPlayer.Chatted:connect(function(msg)
time = string.sub(msg, 9)
 if msg == ".speed" then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
 end
end)

game.Players.LocalPlayer.Chatted:connect(function(msg)
time = string.sub(msg, 9)
 if msg == ".jump" then
 game.Players.LocalPlayer.Character.Humanoid.JumpPower = 100
 end
end)

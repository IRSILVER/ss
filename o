function notify(msg)     game.StarterGui:SetCore("SendNotification", {
Title = "NTO'S ADMIN";         
Text = msg;         
Duration = 3;     
}) 
end
notify("Welcome to NTO'S Admin")
wait(2)
function notify(msg)     game.StarterGui:SetCore("SendNotification", {
Title = "NTO'S ADMIN";         
Text = msg;         
Duration = 3;     
}) 
end
notify("Made for kohls admin house, use .cmds to see commands")

-- commands
game.Players.LocalPlayer.Chatted:connect(function(msg)
time = string.sub(msg, 9)
 if msg == ".admin" then
local pads = game:GetService("Workspace").Terrain["_Game"].Admin.Pads:GetChildren("Head")
 
for i, pad in pairs(pads) do
pad.PrimaryPart = pad:FindFirstChild("Head")
local pos = pad.PrimaryPart.CFrame
wait(0)
pad.PrimaryPart.CanCollide = false
pad:SetPrimaryPartCFrame(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
wait(0)
pad:SetPrimaryPartCFrame(pos)
pad.PrimaryPart.CanCollide = true
end
end
 
end)

game.Players.LocalPlayer.Chatted:connect(function(msg)
time = string.sub(msg, 9)
 if msg == ".clear" or msg == ".clr" then
fireclickdetector(game:GetService("Workspace").Terrain["_Game"].Admin.Regen.ClickDetector)
 end
end)

game.Players.LocalPlayer.Chatted:connect(function(msg)
time = string.sub(msg, 9)
 if msg == ".crash" then
 game:GetService'Players':Chat(("jail all"))
game:GetService'Players':Chat(("fogend 250"))
game:GetService'Players':Chat(("fogcolor 1 1 000"))
game:GetService'Players':Chat(("time 0"))
game:GetService'Players':Chat(("freeze all"))
